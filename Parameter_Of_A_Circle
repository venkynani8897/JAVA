package importjava.util.scanner;

import java.util.Scanner;

public class Parameter_Of_A_Circle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int r;
		double pi=3.14;
		double d;
		Scanner s=new Scanner(System.in);
		System.out.println("Enter any Radius Of The Circle ");
		r=s.nextInt();
		d=2*pi*r;
		System.out.println("The Parameter Of The Circle is: " +" "+d);
		
	}

}
